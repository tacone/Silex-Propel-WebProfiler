{
    "name": "tacone/silex-propel-web-profiler",
    "description": "Extending Silex WebProfiler with Propel information",
    "homepage": "https://github.com/tacone/Silex-Propel-WebProfiler",
    "license": "MIT",
    "authors": [
        {
            "name": "tacone",
            "email": "tacone@gmail.com"
        }
